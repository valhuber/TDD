    "version": "0.2.0",
    "configurations": [
        {
            "name": "ApiLogicServer",
            "type": "python",
            "request": "launch",
            "program": "api_logic_server_run.py",
            "redirectOutput": true,
            "justMyCode": false,
            "args": ["localhost", "5656"],
            "console": "integratedTerminal"
        },
        {
            "name": "Debug Behave Logic",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/test/api_logic_server_behave",
            "program": "${workspaceFolder}/test/api_logic_server_behave/debug_behave.py",
            "redirectOutput": true,
            "justMyCode": false,            
            "console": "integratedTerminal"
        },
        {
            "name": "Report Behave Logic",
            "type": "python",
            "request": "launch",
            "program": "test/api_logic_server_behave/report_behave_logic.py",
            "redirectOutput": true,
            "justMyCode": false,            
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "redirectOutput": true,
            "justMyCode": false,            
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "redirectOutput": true,
            "justMyCode": false,            
            "module": "enter-your-module-name"
        }
    ]
}